{
    "name": "cp-made-easy-vscode",
    "displayName": "",
    "description": "hello",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "ts-node": "^7.0.1",
        "tslint": "^5.8.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.26"
    },
    "dependencies": {
        "@types/request": "^2.48.1",
        "request": "^2.88.0",
        "ts-json-properties": "^2.0.3"
    }
}
